{
  "name": "gcc-arm-linux",
  "displayName": "Arm toolchain Linux",
  "description": "GNU Arm embedded toolchain for Linux (64-bit)",
  "icon": "images/gcc-linux-128x128.png",
  "version": "1.0.2",
  "preview": true,
  "publisher": "chipcode-nl",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "GNU Arm embedded toolchain configuration",
      "properties": {
        "arm-none-eabi.bin": {
          "type": [
            "string",
            "null"
          ],
          "default": "${env:HOME}/.vscode/extensions/chipcode-nl.gcc-arm-linux-1.0.2/bin",
          "description": "The path to the GNU toolchain."
        },
        "arm-none-eabi.include": {
          "type": [
            "string",
            "null"
          ],
          "default": "${env:HOME}/.vscode/extensions/chipcode-nl.gcc-arm-linux-1.0.2/arm-none-eabi/include",
          "description": "The path to the header files."
        },
        "arm-none-eabi.lib": {
          "type": [
            "string",
            "null"
          ],
          "default": "${env:HOME}/.vscode/extensions/chipcode-nl.gcc-arm-linux-1.0.2/lib/gcc/arm-none-eabi/10.2.1",
          "description": "The path to the library files."
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chipcode-nl/gcc-arm-linux.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/vscode": "^1.52.0"
  }
}